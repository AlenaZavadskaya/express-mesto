{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "no-underscore-dangle": [
      {
        "allow": [
          "_id"
        ]
      }
    ],
    "no-console": "off",
    "linebreak-style": "off"
  }
}